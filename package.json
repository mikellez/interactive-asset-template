    "extract:messages": "node src/message-extractor.cjs src/templates/template.js public/content.json",
    "build:copy-contents": "grunt build:copy-contents",
    "build:assets": "grunt build:assets",
    "build:all": "npm run build:copy-contents && npm run build:assets",
